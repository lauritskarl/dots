if status is-interactive
    fish_config theme choose Dracula
    {{ if eq .chezmoi.os "darwin" -}}
    fish_add_path /Users/{{ .chezmoi.username }}/.local/bin/
    fish_add_path /opt/homebrew/bin/
    fish_add_path /opt/homebrew/sbin
    fish_add_path /opt/homebrew/opt/postgresql@18/bin
    source /Users/{{ .chezmoi.username }}/.config/op/plugins.sh
    {{- end }}
    mise activate fish | source
    fzf --fish | source
    zoxide init fish --cmd cd | source
    atuin init fish | source
    starship init fish | source
    thefuck --alias fk | source
    # rv shell init fish | source
    # rv shell completion fish | source
end
